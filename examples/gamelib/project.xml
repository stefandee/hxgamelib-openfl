<?xml version="1.0" encoding="utf-8"?>
<project>	
	<meta title="Gamelib OpenFL HTML5 Test" package="com.pirongames.gamelib" version="1.0.0" company="Piron Games" />
	<app main="GameLibTest" path="export" file="GameLibTest" />
	
	<window orientation="landscape" background="#000000" fps="25" antialiasing="0" resizable="false" />
	<window width="640" height="480" unless="mobile" />
	
	<haxeflag name="-D ImprovedAnimFinished" />
	
	<source path="..\..\src" />
	<source path="src" />
	
	<haxelib name="openfl" />		
	
	<assets path="assets/sprites" rename="sprites">
		<binary id="SpriteData_Spee" path="spee.bin" />
	</assets>
		
	<assets path="assets/strings" rename="strings">
		<binary id="LangData_EN" path="EN/AllStrings.bin" />
		<binary id="LangData_RO" path="RO/AllStrings.bin" />
	</assets>
  
</project>